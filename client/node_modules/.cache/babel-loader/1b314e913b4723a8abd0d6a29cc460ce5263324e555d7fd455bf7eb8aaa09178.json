{"ast":null,"code":"import EmployeesService from \"@/services/EmployeeSevice\";\nexport default {\n  name: \"employees-list\",\n  data() {\n    return {\n      employees: [],\n      currentEmployee: null,\n      currentIndex: -1,\n      name: \"\"\n    };\n  },\n  methods: {\n    retrieveEmployees() {\n      EmployeesService.getAll().then(response => {\n        this.employees = response.data;\n        console.log(response.data);\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    refreshList() {\n      this.retrieveEmployees();\n      this.currentEmployee = null;\n      this.currentIndex = -1;\n    },\n    setActiveEmployee(employee, index) {\n      this.currentEmployee = employee;\n      this.currentIndex = employee ? index : -1;\n    },\n    searchName() {\n      EmployeesService.findByName(this.name).then(response => {\n        this.employees = response.data;\n        this.setActiveEmployee(null);\n        console.log(response.data);\n      }).catch(e => {\n        console.log(e);\n      });\n    }\n  },\n  mounted() {\n    this.retrieveEmployees();\n  }\n};","map":{"version":3,"names":["EmployeesService","name","data","employees","currentEmployee","currentIndex","methods","retrieveEmployees","getAll","then","response","console","log","catch","e","refreshList","setActiveEmployee","employee","index","searchName","findByName","mounted"],"sources":["C:\\projects\\vue-spring\\client\\src\\components\\employees\\EmployeesList.vue"],"sourcesContent":["<template>\r\n  <div class=\"list row\">\r\n    <div class=\"col-md-8\">\r\n      <div class=\"input-group mb-3\">\r\n        <input type=\"text\" class=\"form-control\" placeholder=\"Search by name\"\r\n               v-model=\"name\"/>\r\n        <div class=\"input-group-append\">\r\n          <button class=\"btn btn-outline-secondary\" type=\"button\"\r\n                  @click=\"searchName\"\r\n          >\r\n            Search\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"col-md-6\">\r\n      <h4>Employee List</h4>\r\n      <ul class=\"list-group\">\r\n        <li class=\"list-group-item\"\r\n            :class=\"{ active: index === currentIndex }\"\r\n            v-for=\"(employee, index) in employees\"\r\n            :key=\"index\"\r\n            @click=\"setActiveEmployee(employee, index)\"\r\n        >\r\n          {{ employee.name }}\r\n        </li>\r\n      </ul>\r\n\r\n      <div class=\"nav-item\">\r\n        <router-link to=\"/addEmployee\" class=\"nav-link\">Add</router-link>\r\n      </div>\r\n\r\n    </div>\r\n    <div class=\"col-md-6\">\r\n      <div v-if=\"currentEmployee\">\r\n        <h4>Employee</h4>\r\n        <div>\r\n          <label><strong>Name:</strong></label> {{ currentEmployee.name }}\r\n        </div>\r\n        <div>\r\n          <label><strong>Note:</strong></label> {{ currentEmployee.note }}\r\n        </div>\r\n        <div>\r\n          <label><strong>Profession:</strong></label> {{ currentEmployee.profession }}\r\n        </div>\r\n        <div>\r\n          <label><strong>Department:</strong></label> {{ currentEmployee.department }}\r\n        </div>\r\n        <router-link :to=\"'/employees/' + currentEmployee.id\" class=\"badge badge-warning\">Edit</router-link>\r\n      </div>\r\n      <div v-else>\r\n        <br/>\r\n        <p>Please click on a Employees...</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport EmployeesService from \"@/services/EmployeeSevice\";\r\n\r\nexport default {\r\n  name: \"employees-list\",\r\n  data() {\r\n    return {\r\n      employees: [],\r\n      currentEmployee: null,\r\n      currentIndex: -1,\r\n      name: \"\"\r\n    };\r\n  },\r\n  methods: {\r\n    retrieveEmployees() {\r\n      EmployeesService.getAll()\r\n          .then(response => {\r\n            this.employees = response.data;\r\n            console.log(response.data);\r\n          })\r\n          .catch(e => {\r\n            console.log(e);\r\n          });\r\n    },\r\n    refreshList() {\r\n      this.retrieveEmployees();\r\n      this.currentEmployee = null;\r\n      this.currentIndex = -1;\r\n    },\r\n\r\n    setActiveEmployee(employee, index) {\r\n      this.currentEmployee = employee;\r\n      this.currentIndex = employee ? index : -1;\r\n    },\r\n\r\n    searchName() {\r\n      EmployeesService.findByName(this.name)\r\n          .then(response => {\r\n            this.employees = response.data;\r\n            this.setActiveEmployee(null);\r\n            console.log(response.data);\r\n          })\r\n          .catch(e => {\r\n            console.log(e);\r\n          });\r\n    }\r\n  },\r\n  mounted() {\r\n    this.retrieveEmployees();\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.list {\r\n  text-align: left;\r\n  max-width: 750px;\r\n  margin: auto;\r\n}\r\n</style>\r\n"],"mappings":"AA2DA,OAAOA,gBAAe,MAAO,2BAA2B;AAExD,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,eAAe,EAAE,IAAI;MACrBC,YAAY,EAAE,CAAC,CAAC;MAChBJ,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACDK,OAAO,EAAE;IACPC,iBAAiBA,CAAA,EAAG;MAClBP,gBAAgB,CAACQ,MAAM,CAAC,EACnBC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACP,SAAQ,GAAIO,QAAQ,CAACR,IAAI;QAC9BS,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACR,IAAI,CAAC;MAC5B,CAAC,EACAW,KAAK,CAACC,CAAA,IAAK;QACVH,OAAO,CAACC,GAAG,CAACE,CAAC,CAAC;MAChB,CAAC,CAAC;IACR,CAAC;IACDC,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACR,iBAAiB,CAAC,CAAC;MACxB,IAAI,CAACH,eAAc,GAAI,IAAI;MAC3B,IAAI,CAACC,YAAW,GAAI,CAAC,CAAC;IACxB,CAAC;IAEDW,iBAAiBA,CAACC,QAAQ,EAAEC,KAAK,EAAE;MACjC,IAAI,CAACd,eAAc,GAAIa,QAAQ;MAC/B,IAAI,CAACZ,YAAW,GAAIY,QAAO,GAAIC,KAAI,GAAI,CAAC,CAAC;IAC3C,CAAC;IAEDC,UAAUA,CAAA,EAAG;MACXnB,gBAAgB,CAACoB,UAAU,CAAC,IAAI,CAACnB,IAAI,EAChCQ,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACP,SAAQ,GAAIO,QAAQ,CAACR,IAAI;QAC9B,IAAI,CAACc,iBAAiB,CAAC,IAAI,CAAC;QAC5BL,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACR,IAAI,CAAC;MAC5B,CAAC,EACAW,KAAK,CAACC,CAAA,IAAK;QACVH,OAAO,CAACC,GAAG,CAACE,CAAC,CAAC;MAChB,CAAC,CAAC;IACR;EACF,CAAC;EACDO,OAAOA,CAAA,EAA