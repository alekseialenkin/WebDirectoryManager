{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport DepartmentService from \"../services/DepartmentService\";\nexport default {\n  name: \"department-component\",\n  data() {\n    return {\n      currentDepartment: null,\n      message: ''\n    };\n  },\n  methods: {\n    getDepartment(id) {\n      DepartmentService.get(id).then(response => {\n        this.currentDepartment = response.data;\n        console.log(response.data);\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    updateDepartment() {\n      DepartmentService.update(this.currentDepartment.id, this.currentDepartment).then(response => {\n        console.log(response.data);\n        this.message = 'The department was updated successfully!';\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    deleteDepartment() {\n      DepartmentService.delete(this.currentDepartment.id).then(response => {\n        console.log(response.data);\n        this.$router.push({\n          name: \"departments\"\n        });\n      }).catch(e => {\n        console.log(e);\n      });\n    }\n  },\n  mounted() {\n    this.message = '';\n    this.getDepartment(this.$route.params.id);\n  }\n};","map":{"version":3,"names":["DepartmentService","name","data","currentDepartment","message","methods","getDepartment","id","get","then","response","console","log","catch","e","updateDepartment","update","deleteDepartment","delete","$router","push","mounted","$route","params"],"sources":["C:\\projects\\vue-spring\\client\\src\\components\\departments\\Department.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"currentDepartment\" class=\"edit-form\">\r\n    <h4>Department</h4>\r\n    <form>\r\n      <div class=\"form-group\">\r\n        <label for=\"name\">Name</label>\r\n        <input type=\"text\" class=\"form-control\" id=\"name\"\r\n               v-model=\"currentDepartment.name\"\r\n        />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"note\">